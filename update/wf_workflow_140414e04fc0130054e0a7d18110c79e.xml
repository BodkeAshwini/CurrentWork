        <value>workflow.scratchpad.result = ifScript();&#13;
function ifScript(){&#13;
	var result = {red:false, blue:false, green:false, none:false};&#13;
	if (workflow.scratchpad.trigger){&#13;
		if (workflow.scratchpad.trigger.indexOf('red') &gt; -1){&#13;
			result.red=true;&#13;
		}&#13;
		if (workflow.scratchpad.trigger.indexOf('blue') &gt; -1){&#13;
			result.blue=true;&#13;
		}&#13;
		if (workflow.scratchpad.trigger.indexOf('green') &gt; -1){&#13;
			result.green=true;&#13;
		}&#13;
		&#13;
	}&#13;
	result.none = (!(result.red || result.blue || result.green));&#13;
	return result;&#13;
}&#13;
&#13;
</value>
